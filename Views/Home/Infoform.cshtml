                                        </div>
                                        <div class="form-group col-3">
                                            <label >نوع المعدل</label>
                                            <input disable  value="@Model.GPAScale" class="form-control form-control-border">
                                         
                                        </div>
                                        <div class="form-group col-3">
                                            <label >المعدل</label>
                                            <input disable  value="@Model.GPA" class="form-control form-control-border">
                                         
                                        </div>
                                        <div class="form-group col-3">
                                            <label >الجامعة</label>
                                            <input disable  value="@Model.University" class="form-control form-control-border">
                                         
                                        </div>
                                        <div class="form-group col-3">
                                            <label >سنة التخرج</label>
                                            <input disable type="date" placeholder="YYYY"  value="@Model.GraduationYear" class="form-control form-control-border">
                                        
                                        </div>
                                        <div class="form-group col-3">
                                            <label >الحالة الوظيفية</label>
                                            <select disable  value="@Model.EmployeeStatus" class="form-control form-control-border">
                                                <option value="موظف">موظف</option>
                                                <option value="غير موظف">غير موظف</option>
                                            </select>
                                          
                                        </div>
                                        <div class="form-group col-3">
                                            <label >سنوات الخبرة</label>
                                            <input disable  value="@Model.ExpYears" class="form-control form-control-border">
                                         
                                        </div>
                                        <div class="form-group col-3">
                                            <label >مستوى اللغة الإنجليزية</label>
                                            <select disable  value="@Model.EnglishLevel" class="form-control form-control-border">
                                                <option value="متوسط">متوسط</option>
                                                <option value="متقدم">متقدم</option>
                                            </select>
                                         
                                        </div>
                                        <div class="form-group col-3">
                                            <label >سبب التقديم على المبادرة</label>
                                            <input disable  value="@Model.RegistrationReason" class="form-control form-control-border">
                                          
                                        </div>
                                        <div class="form-group col-3">
                                            <label >متفرغ</label>
                                            <select disable  value="@Model.FreementStaus" class="form-control form-control-border">
                                                <option value="نعم">نعم</option>
                                                <option value="لا">لا</option>
                                            </select>
                                          
                                        </div>
                                        <input disable  value="@Model.Status" class="form-control form-control-border" style="display: none;">



                                        <div class="form-group col-3">
                                            <a type="button" class="btn btn-block btn-primary" asp-area="" asp-controller="Home" asp-action="OpenFile" asp-route-TraineeID="@Model.NationalID" asp-route-filetype="CV">عرض السيرة الذاتية</a>
                                        </div>
                                        <div class="form-group col-3">
                                            <a type="button" class="btn btn-block btn-primary" asp-area="" asp-controller="Home" asp-action="OpenFile" asp-route-TraineeID="@Model.NationalID" asp-route-filetype="Certificate">عرض الشهادة </a>

                                        </div>
                                        <div class="form-group col-3">
                                            <<a type="button" class="btn btn-block btn-primary" asp-area="" asp-controller="Home" asp-action="OpenFile" asp-route-TraineeID="@Model.NationalID" asp-route-filetype="IntroVideo">عرض الفيديو التعريفي </a>

                                        </div>
                                        <div class="form-group col-3">
                                           <a type="button" class="btn btn-block btn-primary" asp-area="" asp-controller="Home" asp-action="OpenFile" asp-route-TraineeID="@Model.NationalID" asp-route-filetype="PreviosWork">عرض المشاريع السابقة</a>
                                        </div>


                                    </div> <div class="card-footer">
                                        <a asp-area="" asp-controller="Home" asp-action="InfoDashboard" class="btn btn-primary">عودة</a>
                                    </div>
                                </div>
