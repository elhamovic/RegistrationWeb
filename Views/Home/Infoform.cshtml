                                                <option value="كندا">كندا</option>
                                                <option value="استرليا">استرليا</option>
                                            </select>
                                         
                                        </div>
                                        <div class="form-group col-3">
                                            <label >رقم الهوية</label>
                                            <input disable  value="@Model.NationalID" class="form-control form-control-border">
                                          
                                        </div>
                                        <div class="form-group col-3">
                                            <label >تاريخ الميلاد</label>
                                            <input disable type="date" placeholder="MM/DD/YYYY"  value="@Model.DateOfBirth" class="form-control form-control-border">
                                           
                                        </div>
                                        <div class="form-group col-3">
                                            <label >الجنس</label>
                                            <select disable  value="@Model.Gender" class="form-control form-control-border">
                                                <option value="ذكر">ذكر</option>
                                                <option value="أنثى">أنثى</option>
                                            </select>
                                          
